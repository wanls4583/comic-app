.navigation_bar {
    position: fixed;
    top: 0;
    z-index: 99;
    width: 100%;
    color: #fff;
    box-sizing: content-box;
}

.navigation_bar .title_w {
    display: flex;
    align-items: center;
    position: relative;
    height: 100%;
    padding-right: 100px;
}

.title {
    color: #fff;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.icon_back {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0px;
    width: 40px;
    height: 100%;
    background: url('data:image/png;base64,');
    background-size: 20px 20px;
    background-position: center;
    background-position-x: 5px;
    background-repeat: no-repeat;
}

.navigation_bar_holder {
    background-color: transparent;
}